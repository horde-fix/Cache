        "horde/hashtable": "^1",
        "horde/log": "^2",
        "horde/memcache": "^2",
        "horde/mongo": "^1",
        "ext-APC": "^3.1.1",
        "ext-eaccelerator": "0.9.5",
        "ext-xcache": "*"
